{
  "name": "create-react-matt",
  "version": "0.0.13",
  "description": "React, Redux, Webpack, Babel, Jest, and code coverage all provided for you",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcrowder65/create-react-matt"
  },
  "bin": {
    "create-react-matt": "./lib/src/index.js"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "linter": "eslint src && eslint test",
    "build": "rm -rf lib && babel src/index.js -d lib",
    "test": "npm run linter && jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "src",
    "test",
    ".babelrc",
    ".eslintrc",
    ".gitignore",
    "package.json",
    "webpack.config.js"
  ],
  "keywords": [
    "react",
    "redux",
    "react router",
    "webpack",
    "jest"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js*}",
      "!src/client/app.jsx",
      "!src/client/router.jsx",
      "!src/client/actions/sagas/config.jsx",
      "!src/client/actions/sagas/index.jsx"
    ]
  },
  "eslintConfig": {
    "extends": [
      "mcrowder65"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^2.15.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chalk": "^2.3.2",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "eslint-config-mcrowder65": "^0.0.16",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.1.1",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
